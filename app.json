{
  "expo": {
    "name": "Shutterspace",
    "slug": "Shutterspace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "shutterspace",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": false,
      "bundleIdentifier": "org.bbtechnologies.shutterspace",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "org.bbtechnologies.shutterspace",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-google-signin/google-signin",
      "expo-apple-authentication",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Shutterspace needs access to your photos to let you share them.",
          "savePhotosPermission": "The app needs access to your photos to let you save them.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Shutterspace needs access to your camera to let you take photos.",
          "microphonePermission": "Shutterspace needs access to your microphone to let you take videos.",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SansitaOne.ttf"
          ]
        }
      ],
      "expo-video",
      [
        "expo-image-picker",
        {
          "photosPermission": "Shutterspace needs access to your photos to let you share them.",
          "cameraPermission": "Shutterspace needs access to your camera to let you take photos and videos."
        }
      ],
      "react-native-compressor"
    ],
    "experiments": {
      "turbomodule": true
    },
    "extra": {
      "eas": {
        "projectId": "6b2ea930-bc68-4cea-9720-6f4df870d758"
      }
    }
  }
}